                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{S rSub { size 8{1} }  \( t \) =V sqrt { ital "tb"}  "." U rSub { size 8{1} }  \( t \) } {}</m:annotation>
            </m:semantics>
          </m:math>
        </equation>
