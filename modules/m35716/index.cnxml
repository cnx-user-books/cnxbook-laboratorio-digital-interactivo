</m:mtd>
                    </m:mtr>
                    <m:mtr>
<m:mtd>
                      <m:mrow/>
</m:mtd>
                    </m:mtr>
                    <m:mtr>
<m:mtd>
                      <m:mrow>
                        <m:mstyle fontstyle="italic">
                          <m:mrow>
                            <m:mtext>Gy</m:mtext>
                          </m:mrow>
                        </m:mstyle>
                        <m:mo stretchy="false">(</m:mo>
                        <m:mi>f</m:mi>
                        <m:mrow>
                          <m:mo stretchy="false">)</m:mo>
                          <m:mo stretchy="false">=</m:mo>
                          <m:msup>
                            <m:mfrac>
                              <m:mi>V</m:mi>
