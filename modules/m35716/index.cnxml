                        </m:mstyle>
                        <m:mrow/>
                      </m:mrow>
</m:mtd>
                    </m:mtr>
                    <m:mtr>
<m:mtd>
                      <m:mrow>
                        <m:msub>
                          <m:mi>S</m:mi>
                          <m:mstyle fontsize="8pt">
                            <m:mrow>
                              <m:mn>2</m:mn>
                            </m:mrow>
                          </m:mstyle>
                        </m:msub>
