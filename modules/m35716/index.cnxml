              <m:annotation encoding="StarMath 5.0">alignl { stack {
 size 12{ ital "Gx" \( f \) =F left lbrace  ital "Rx" \( τ \)  right rbrace }  {} # 
 size 12{ ital "Rx" \( τ \) = Sum { {  { ital "Rn" "." }  over  { ital "Tb"} } δ \( τ+ ital "nTb" \) } }  {} # 
 size 12{ ital "Rn"="lim" rSub { size 8{T rightarrow  infinity } }  {  { ital "tb"}  over  {T} }  Sum cSub { size 8{k} }  {A rSub { size 8{k} }  "." A rSub { size 8{ \( n+k \) } } } }  {} 
} } {}</m:annotation>
