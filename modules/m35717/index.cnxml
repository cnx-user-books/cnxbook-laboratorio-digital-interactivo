                                    <m:mrow>
                                      <m:mn>2</m:mn>
                                    </m:mrow>
                                  </m:mstyle>
                                </m:msup>
                              </m:mrow>
                            </m:mstyle>
                            <m:mrow>
                              <m:mfenced open="(" close=")">
